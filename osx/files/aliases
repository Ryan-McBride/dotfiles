#git aliases
alias ga='git add'
alias gs='git status'
alias gii='git commit'
alias gp='git pull'
alias go='git checkout'
alias gd='git diff'
alias grh='git reset HEAD~'
alias gh='git log --decorate --graph --oneline'

#directory aliases
alias work='cd ~/Workspace'
alias vrc='v ~/.config/nvim/init.vim'
#
#program aliases
alias v='nvim'
alias prof='nvim -p ~/.zshrc ~/.aliases ~/.computer-specific-aliases'
alias rprof='. ~/.zshrc'


#other stuff
unset file
export EDITOR=nvim
